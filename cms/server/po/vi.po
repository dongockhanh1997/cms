"hỏi được trả lời"

#, python-format
msgid ""
"You have reached the maximum limit of at most %d submissions among all tasks."
msgstr "Bạn đã sử dụng hết %d lần nộp bài cho phép áp dụng cho tất cả các bài."

#, python-format
msgid ""
"You have reached the maximum limit of at most %d submissions on this task."
msgstr "Bạn đã sử dụng hết %d lần nộp bài cho phép áp dụng riêng với bài này."

msgid "Too many submissions!"
msgstr "Quá nhiều bài nộp!"

#, python-format
msgid ""
"Among all tasks, you can submit again after %d seconds from last submission."
msgstr ""
"Trong tất cả các bài nộp, hai bài nộp liên tiếp phải cách nhau ít nhất %d "
"giây."

#, python-format
msgid ""
"For this task, you can submit again after %d seconds from last submission."
msgstr ""
"Trong tất cả các bài nộp cho bài này, hai bài nộp liên tiếp phải cách nhau "
"ít nhất %d giây."

msgid "Submissions too frequent!"
msgstr "Các bài nộp quá gần nhau!"

msgid "Invalid submission format!"
msgstr "Định dạng bài nộp không hợp lệ!"

msgid "Please select the correct files."
msgstr "Hãy chọn các tập tin một cách chính xác."

msgid "Invalid archive format!"
msgstr "Định dạng tập tin nén không hợp lệ!"

msgid "The submitted archive could not be opened."
msgstr "Không thể mở tập tin nén đã nộp."

msgid "Cannot recognize submission's language."
msgstr "Không thể nhận diện ngôn ngữ lập trình được sử dụng trong bài nộp."

msgid "All sources must be in the same language."
msgstr "Tất cả các tập tin mã nguồn phải sử dụng chung một ngôn ngữ lập trình."

#, python-format
msgid "Language %s not allowed in this contest."
msgstr "Ngôn ngữ %s không được phép sử dụng trong kì thi này."

msgid "Invalid submission!"
msgstr "Bài nộp không hợp lệ!"

msgid "Submission too big!"
msgstr "Kích thước bài nộp quá lớn!"

#, python-format
msgid "Each source file must be at most %d bytes long."
msgstr "Kích thước mỗi tập tin mã nguồn không được vượt quá %d bytes."

msgid "Submission storage failed!"
msgstr "Quá trình lưu trữ bài nộp gặp lỗi!"

msgid "Please try again."
msgstr "Hãy thử lại."

msgid "Submission received"
msgstr "Bài nộp đã được tiếp nhận"

msgid "Your submission has been received and is currently being evaluated."
msgstr "Bài nộp đã được tiếp nhận và đang được chấm."

msgid "Token request discarded"
msgstr "Yêu cầu sử dụng token bị từ chối"

msgid "Your request has been discarded because you have no tokens available."
msgstr "Yêu cầu sử dụng token bị từ chối vì bạn không còn token nào."

msgid ""
"Your request has been discarded because you already used a token on that "
"submission."
msgstr ""
"Yêu cầu sử dụng token bị từ chối vì bạn đã sử dụng token cho bài nộp đó."

msgid "Token request received"
msgstr "Yêu cầu sử dụng token đã được tiếp nhận"

msgid "Your request has been received and applied to the submission."
msgstr "Yêu cầu sử dụng token đã được tiếp nhận và được sử dụng cho bài nộp."

msgid "Compiling..."
msgstr "Đang biên dịch..."

msgid "details"
msgstr "chi tiết"

msgid "Evaluating..."
msgstr "Đang chấm..."

msgid "Scoring..."
msgstr "Đang tính điểm..."

msgid "Evaluated"
msgstr "Đã chấm xong"

#, python-format
msgid "You have reached the maximum limit of at most %d tests among all tasks."
msgstr ""
"Bạn đã sử dụng hết %d lần chạy thử cho phép áp dụng cho tất cả các bài."

#, python-format
msgid "You have reached the maximum limit of at most %d tests on this task."
msgstr "Bạn đã sử dụng hết %d lần chạy thử cho phép áp dụng riêng cho bài này."

msgid "Too many tests!"
msgstr "Quá nhiều lần chạy thử!"

#, python-format
